# Aptos Network Configuration
VITE_APTOS_NETWORK=testnet
VITE_APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1
VITE_CONTRACT_ADDRESS=0x1

# Iagon Decentralized Storage Configuration
# Get your API key from https://iagon.com/developers
VITE_IAGON_API_KEY=your_iagon_api_key_here
VITE_IAGON_ENDPOINT=https://api.iagon.com/v1
VITE_IAGON_NETWORK=testnet

# Security Settings
VITE_ENABLE_ENCRYPTION=true
VITE_STORAGE_REDUNDANCY=3

# Development Settings
VITE_DEV_MODE=true
VITE_ENABLE_FALLBACK_STORAGE=true